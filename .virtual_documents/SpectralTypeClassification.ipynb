from astropy.io import fits








with fits.open('daten_praxis/1.fits') as file:
    header = file[0].header
    data = file[0].data
print(header)








print("NAXIS:", header["NAXIS"])
print("NAXIS1:", header["NAXIS1"])
print("WCSDIM:", header["WCSDIM"])
print("CTYPE1:", header["CTYPE1"])
