wavelength_end = header["CRVAL1"]+(header["NAXIS1"]-1)*header["CDELT1"]
print(wavelength_end)





x = np.arange(header["CRVAL1"], wavelength_end + header["CDELT1"], header["CDELT1"])
y = data






print("x:", len(x))
print(f"y: {len(y)}")






yMin = 0
yMax = 2.75

fig = plt.figure(figsize=(10, 8))
